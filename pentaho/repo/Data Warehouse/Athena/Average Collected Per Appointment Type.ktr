  <step>
    <name>Lookup Average Charges</name>
    <type>DBJoin</type>
    <description />
    <distribute>Y</distribute>
    <custom_distribution />
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name />
    </partitioning>
    <connection>Data Warehouse</connection>
    <rowlimit>0</rowlimit>
    <sql>SELECT AVG(charge) * -1 'average_payment'
FROM Appointment a 
JOIN 
  (
    SELECT patient_id, date_of_service, SUM(amount) charge 
    FROM `Transaction` WHERE type = "PAYMENT" 
    AND code NOT LIKE "MISC%"
    GROUP BY patient_id, date_of_service
  ) charges
ON a.patient_id = charges.patient_id 
AND DATE(a.start) = DATE(charges.date_of_service)
JOIN 
  (
    SELECT a.patient_id, a.start 
    FROM Appointment a 
    WHERE cancelled IS NULL 
    GROUP BY a.patient_id, DATE(start) 
    HAVING COUNT(a.id) = 1
  ) no_dup
ON a.patient_id = no_dup.patient_id 
AND a.start = no_dup.start
WHERE a.appointment_type = ?
AND a.cancelled IS NULL
AND DATE(a.start) BETWEEN DATE(? - INTERVAL 2 MONTH) AND DATE(? - INTERVAL 1 MONTH)
AND a.site_id = ?</sql>
    <outer_join>N</outer_join>
    <replace_vars>N</replace_vars>
    <parameter>
