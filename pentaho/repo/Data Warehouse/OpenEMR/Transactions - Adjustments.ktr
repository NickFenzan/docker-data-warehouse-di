    <connection>OpenEMR</connection>
    <sql>SELECT ar.pid, real_provider.npi, site.city, ar.code, 
e.date, ar.post_time, ar.payer_type, i.name, ar.adj_amount
FROM ar_activity ar
JOIN ar_session ars
ON ar.session_id = ars.session_id
JOIN form_encounter e
ON ar.encounter = e.encounter
JOIN users provider
ON e.provider_id = provider.id
JOIN users real_provider
ON provider.linkToProvider = real_provider.id
JOIN facility site
ON e.facility_id = site.id
LEFT JOIN insurance_companies i
ON ars.payer_id = i.id
WHERE adj_amount > 0</sql>
